Table customers {
  customer_id int [pk]
  name varchar
  region varchar
}

Table products {
  product_id int [pk]
  product_name varchar
  unit_price decimal
  category varchar
}

Table shipments {
  shipment_id int [pk]
  shipping_method varchar
}

Table orders {
  order_id int [pk]
  customer_id int [ref: > customers.customer_id]
  product_id int [ref: > products.product_id]
  shipment_id int [ref: > shipments.shipment_id]
  order_date date
  ship_date date
  delivery_date date
  expected_delivery_date date
  on_time char
}
        INT product_id FK
        INT shipment_id FK
        DATE order_date
        DATE ship_date
        DATE delivery_date
        DATE expected_delivery_date
        CHAR on_time
    }

